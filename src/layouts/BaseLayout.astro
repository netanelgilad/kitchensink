---
import "../styles/global.css";
type Props = {
    bodyProps?: astroHTML.JSX.HTMLAttributes;
};
---

<!doctype html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <slot name="head" />
    </head>
    <body {...Astro.props.bodyProps}>
        <slot name="body" />
    </body>
</html>
